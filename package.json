{
  "name": "three0-js-sdk",
  "version": "0.1.0",
  "description": "js-sdk for Three0 platform",
  "source": [
    "src/index.js",
    "src/auth/index.js",
    "src/database/index.js"
  ],
  "targets": {
    "default": {
      "includeNodeModules": false,
      "distDir": "dist",
      "isLibrary": true,
      "outputFormat": "esmodule"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./auth": "./dist/auth/index.js",
    "./database": "./dist/database/index.js"
  },
  "scripts": {
    "build": "parcel build",
    "watch": "parcel watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/**/*.js' --no-error-on-unmatched-pattern",
    "lint:fix": "eslint 'src/**/*.js' --no-error-on-unmatched-pattern --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Three0Dev/JS-SDK.git"
  },
  "author": "Three0",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Three0Dev/JS-SDK/issues"
  },
  "homepage": "https://github.com/Three0Dev/JS-SDK#readme",
  "dependencies": {
    "@swc/helpers": "^0.4.3",
    "@types/orbit-db": "github:orbitdb/orbit-db-types",
    "ipfs-core": "^0.15.4",
    "js-sha256": "^0.9.0",
    "near-api-js": "^0.45.1",
    "orbit-db": "^0.28.6",
    "orbit-db-identity-provider": "^0.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 12.0.0"
  }
}
