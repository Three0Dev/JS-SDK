{
  "name": "@three0dev/js-sdk",
  "version": "0.1.0-alpha",
  "description": "JS-SDK for Three0 platform",
  "author": "Three0",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "test": "jest --verbose",
    "lint": "eslint 'src/**/*.js' --no-error-on-unmatched-pattern",
    "lint:fix": "eslint 'src/**/*.js' --no-error-on-unmatched-pattern --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Three0Dev/JS-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/Three0Dev/JS-SDK/issues"
  },
  "homepage": "https://github.com/Three0Dev/JS-SDK#readme",
  "dependencies": {
    "@swc/helpers": "^0.4.3",
    "@types/orbit-db": "github:orbitdb/orbit-db-types",
    "ipfs-core": "^0.14.3",
    "near-api-js": "^0.45.1",
    "orbit-db": "^0.28.6",
    "orbit-db-identity-provider": "^0.4.0",
    "orbit-db-io": "1.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.1",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 12.0.0"
  },
  "source": [
    "src/index.js",
    "src/auth/index.js",
    "src/database/index.js"
  ],
  "targets": {
    "module": {
      "includeNodeModules": false,
      "distDir": "dist/module",
      "isLibrary": true,
      "outputFormat": "esmodule"
    },
    "common": {
      "includeNodeModules": false,
      "distDir": "dist/common",
      "isLibrary": true,
      "outputFormat": "commonjs"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/module/index.js",
      "require": "./dist/common/index.js"
    },
    "./auth": {
      "import": "./dist/module/auth/index.js",
      "require": "./dist/common/auth/index.js"
    },
    "./database": {
      "import": "./dist/module/database/index.js",
      "require": "./dist/common/database/index.js"
    }
  }
}
